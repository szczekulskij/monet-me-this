    "import time\n",
    "\n",
    "current_time = datetime.datetime.now()\n",
    "milliseconds = int(time.mktime(current_time.timetuple())) * 1000\n",
    "stringed = str(milliseconds + current_time.microsecond)\n",
    "print(stringed)"
